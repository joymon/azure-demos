<configuration>
  <appSettings>
    <!--Normally the service account name looks like an email id. eg: <user1>@<aadtenant>.onmicrosoft.com-->
    <add key="serviceAccountName" value="" />
    <add key="serviceAccountPasswordClearText" value="" />
    <add key="AADTenantId" value="" />
    <add key="AADAppregistrationId" value="" />
    <add key="RootSiteURL" value=""/>

    <!--To work with GraphAPI-->
    <!--GraphAPI works with {hostname},{spsite.id},{spweb.id} format.
    Refer https://docs.microsoft.com/en-us/graph/api/resources/sharepoint?view=graph-rest-1.0#note-for-existing-sharepoint-developers
    Get that special format using https://stackoverflow.com/questions/45406451-->
    <add key="SiteId" value="" />
    <!--Below 2 ids are not GUIDs.-->
    <add key="LibraryId" value="" />
    <add key="FileId" value="" />
    <add key="DownloadBasePath" value="c:\temp" />
    <!--To work with PnPFrameworkLibrary-->
    <!--Below can be obtained from the file properties->Find the hierarchical path then click on copy button in UI-->
    <add key="HumanReadableAbosolutePathToFile" value=""/>
    <add key="SubSiteURL" value=""/>
  </appSettings>
